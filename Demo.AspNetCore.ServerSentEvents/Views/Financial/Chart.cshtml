@model CompositeModel
@using Microsoft.AspNetCore.Identity


@inject SignInManager<IdentityUser> SignInManager

@if (SignInManager.IsSignedIn(User))
{
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h2>
            <i class="fa fa-bar-chart-o"></i> Volatility Strike by Asset:
        </h2>
        <!-- sidebar menu: : style can be found in sidebar.less -->





        <select id="AssetsMenu" class="sidebar-menu"
                asp-items="@(new SelectList(Model.assetModel.AssetList, "Id", "Name"))"
                onclick="OnAssetSelected(@Model.optionsModel.OptionsJsonString)"></select>






        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">
                Assets
            </li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">

        @*<button id="add_tab">Add Tab</button>*@
        <div id="tabs">
            <ul>
                <!--<li><a href="#tabs-1">tab 1</a> <span class="ui-icon ui-icon-close" role="presentation">Remove Tab</span></li>
                -->
            </ul>

        </div>

    </section>
    <!-- /.content -->
}
else
{
    <h3>To use this web site resources, 
        <a id="loginLink" asp-controller="Account" asp-action="Login">log in</a> 
        or 
        <a id="registerLink" asp-controller="Account" asp-action="Register">register</a></h3>
}